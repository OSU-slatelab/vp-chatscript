topic: ~pastsex keep repeat (~groin_male ~sex_problems_male ~groin_female ~sex_problems_female ~contraceptives sex sexual sexually intercourse ~STI infection rape force)


#	u: ($present) respond(~currentsex) ^fail(TOPIC)    
	u: ($family) respond(~familysex) ^fail(TOPIC)



# SEXUAL HEALTH


#! Did it ever hurt during intercourse
	u: Did_it_ever_hurt_during_intercourse (did it ever hurt during intercourse )    ^factanswer("It never has.")
	u: (<< [hurt painful bother ~pain] [sex intercourse "sexual activity"] >>) ^reuse (Did_it_ever_hurt_during_intercourse)
	u: ([sex intercourse "sexual activity"] * [increase exacerbate] * [~the_pain $relevantpain?]) ^reuse (Did_it_ever_hurt_during_intercourse)

#! Did you ever have bleeding after intercourse
	u: Ever_have_bleeding_after_intercourse (did you ever have bleeding after intercourse)    ^factanswer("Never.")
	u: ([blood bleeding bleed] * [sex intercourse "sexual activity"]) ^reuse (Ever_have_bleeding_after_intercourse)

#! Who were your past sexual partners
	u: Who_were_your_past_sexual_partners (who were your past sexual partners)	^factanswer("I have never been sexually active.")
	u: ([who whom] * [sex sexually intercourse sexual]) ^reuse (Who_were_your_past_sexual_partners)
	u: (partner * [man male woman female]) ^reuse (Who_were_your_past_sexual_partners)

#! Any other past sexual partners
	u: Any_other_past_sexual_partners (any other past sexual partners)  ^factanswer("I have never been sexually active.")
	u: ([new other additional extra any] * [sex sexual])  ^reuse (Any_other_past_sexual_partners)
	u: (other * [sex sexual] * [~spouse])  ^reuse (Any_other_past_sexual_partners)
	u: (other * partners) ^reuse (Any_other_past_sexual_partners)

#! Ever been sexually active
	u: Ever_been_sexually_active (ever been sexually active) 	^factanswer("I have never been sexually active.")
	u: (![when why how problem difficulty who ~STI unprotected symptoms force coerce] [ever previously previous prior] * [sex sexual sexually intercourse "sexually active"]) ^reuse (Ever_been_sexually_active)

#! Have you ever had a sexually transmitted infection
	u: Ever_had_an_STI (have you ever had a sexually transmitted infection)	  ^factanswer("Never.")
	u: ([ever had history] * [prior previous past] * [~STI]) ^reuse (Ever_had_an_STI)
	u: ([ever had history] * [~STI]) ^reuse (Ever_had_an_STI)
	u: ([ever had history] * [~STI] * [prior previously before past]) ^reuse (Ever_had_an_STI)

#! When did you have past sexually transmitted infection
	u: When_had_past_STI (when did you have past sexually transmitted infection) ^factanswer("I have never had any.")
	u: ([when "how long ago"] * [~STI infection]) ^reuse (When_had_past_STI)
		 
#! Did you take anything for the past infection
	u: Take_anything_for_the_past_infection (take anything for the past infection) ^factanswer("I never had one of those.")
	u:([take prescribed given treatment ~medicines used] * [~STI infection])  ^reuse (Take_anything_for_the_past_infection)

#! Have you ever been raped
	u: Have_you_ever_been_raped (have you ever been raped) ^factanswer("No, never.")
	u: ([rape forced]) ^reuse (Have_you_ever_been_raped)

#! Tell me about your past sexual history
	u: Tell_me_about_your_past_sexual_history (tell me about your past sexual history) ^factanswer("What do you want to know?")
	u: ([tell describe] * sexual history) ^reuse (Tell_me_about_your_past_sexual_history)


# CONTRACEPTION

#! Did you use any other past contraception
	u: Did_you_use_any_other_past_contraception (did you use any other past contraception) 	^factanswer("No.")
	u: (use * other * ~contraceptives * [before past previously]) ^reuse (Did_you_use_any_other_past_contraception)

#! Did you use any protection every time before
	u: Did_you_use_protection_every_time_before (did you use {any} protection every time before)     ^factanswer("No.")
	u: ([always "every time"] * ~contraceptives [before past previously]) ^reuse (Did_you_use_protection_every_time_before)
	
#! Have you had unprotected sex in the past
	u: Have_you_had_unprotected_sex_in_the_past (have you had unprotected sex in the past)	^factanswer("No.")
	u: (unprotected [sex intercourse] * [before past previously]) ^reuse (Have_you_had_unprotected_sex_in_the_past)
	u: ([sex intercourse] * without * ~contraceptives * [before past previously]) ^reuse (Have_you_had_unprotected_sex_in_the_past)

#! Did you use any contraception in the past
	u: Use_any_contraception_in_the_past (did you use any contraception in the past)    ^factanswer("No.")
	u: (!other !additional ~contraceptives) ^reuse (Use_any_contraception_in_the_past)


