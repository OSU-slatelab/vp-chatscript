# This is the Vaccine Section

topic: ~vaccines keep repeat (vaccine vaccinate vaccinated vaccination vaccinations shot booster up-to-date "up to date" immunized immunization check)

	u: ($family) ^fail(TOPIC)


#! Have you been vaccinated for MMR

	u: have_you_been_vaccinated_for_MMR (have you been vaccinated for MMR) ^factanswer("I have.")
	u: (<<[vaccine vaccines vaccinated vaccination vaccinations][MMR mumps measles rubella]>>) ^reuse(have_you_been_vaccinated_for_MMR)
	u: ([MMR mumps measles rubella]) ^reuse(have_you_been_vaccinated_for_MMR)

#! Have you been vaccinated for hib

	u: have_you_been_vaccinated_for_HIB  (have you been vaccinated for hib)  ^factanswer("I believe so.")
	u: (<<[vaccine vaccines vaccinated vaccination vaccinations]["Haemophilus Influenza b" hib]>>) ^reuse(have_you_been_vaccinated_for_HIB)
	u: (["Haemophilus Influenza b" hib]) ^reuse(have_you_been_vaccinated_for_HIB)

#! Have you been vaccinated for Hepatitis A

	u: have_you_been_vaccinated_for_hepatitis_a (have you been vaccinated for "Hepatitis a")  ^factanswer("I received that vaccine.")
	u: (<<[vaccine vaccines vaccinated vaccination vaccinations]["Hepatitis A" "hep A"]>>) ^reuse(have_you_been_vaccinated_for_hepatitis_a)
	u: (["Hepatitis A" "hep A"]) ^reuse(have_you_been_vaccinated_for_hepatitis_a)

#! Have you been vaccinated for Hepatitis B

	u: have_you_been_vaccinated_for_hepatitis_b (have you been vaccinated for "Hepatitis b")  ^factanswer("I received that vaccine.")
	u: (<<[vaccine vaccines vaccinated vaccination vaccinations]["Hepatitis B" "hep B"]>>) ^reuse(have_you_been_vaccinated_for_hepatitis_B)
	u: (["Hepatitis B" "hep B"]) ^reuse(have_you_been_vaccinated_for_hepatitis_B)

#! Have you been vaccinated for Hepatitis

	u: have_you_been_vaccinated_for_hepatitis (have you been vaccinated for hepatitis) ^factanswer("I think so")
	u: (<<[vaccine vaccines vaccinated vaccination vaccinations] hepatitis >>) ^reuse (have_you_been_vaccinated_for_hepatitis_b)
	u: (hepatitis) ^reuse (have_you_been_vaccinated_for_hepatitis)

#! Have you had the hpv vaccine

	u: have_you_had_the_hpv_vaccine (have you had the hpv vaccine) ^factanswer("I did  not get that.")
	u: (<<[vaccine vaccines vaccinated vaccination vaccinations][hpv "human papilloma virus"]>>) ^reuse(have_you_had_the_hpv_vaccine)
	u: ([hpv "human papilloma virus"]) ^reuse(have_you_had_the_hpv_vaccine)

#! Have you had your flu shot

	u: have_you_had_flu_shot (have you had your flu shot)  ^factanswer("I get one every year.")
	u: (<<[flu influenza][vaccine shot vaccines vaccination vaccinated]>>) ^reuse(have_you_had_flu_shot)

#! Have you had the pneumonia vaccine

	u: have_you_had_the_pneumonia_vaccine (have you had the pneumonia vaccine)  ^factanswer("I haven't had that")
	u: (<<[pneumonia pneumococcal][vaccine shot vaccines vaccination vaccinated]>>) ^reuse(have_you_had_the_pneumonia_vaccine)

#! Have you received your shingles vaccine

	u: Have_you_received_your_shingles_vaccine (have you received your shingles vaccine)   ^factanswer("Not yet.")
	u: (<< shingles [vaccine shot vaccines vaccination vaccinated]>>) ^reuse(have_you_received_your_shingles_vaccine)

#! When was your last tetanus shot

	u: When_was_your_last_tetanus_shot (when was your last tetanus shot)    ^factanswer("I had my last booster within the last 5 years.")
	u: (<<tetanus [vaccine shot vaccines vaccination vaccinated booster]>>) ^reuse(when_was_your_last_tetanus_shot)

#! Are you current on vaccinations
	u: Are_you_current_on_vaccinations (are you current on vaccinations)  ^factanswer("I am up to date.")
	u: ([current recent recently had up-to-date "up to date" all been] * [vaccine vaccination immunized immunization immunizations shots]) ^reuse(are_you_current_on_vaccinations)
	u: ([vaccine vaccination immunization immunizations shots] * [current recent recently had up-to-date "up to date" all]) ^reuse(are_you_current_on_vaccinations)
	u: ([vaccine vaccination immunization immunizations shots]>) ^reuse(are_you_current_on_vaccinations)

