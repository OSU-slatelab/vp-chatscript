# This is the mental health section note - we have included past and family mental health questions here instead of having separate topics for those.

topic: ~mentalhealth keep repeat (depressed depression blue mood moody stress sad lonely suicide suicidal harm intend mental cry anxiety image self "self image" down hopeless worry hurt distracted concentrate think "panic attack" tearful teary "tear up" "not worth living" "better off dead" empty helpless worthless guilt guilty blame happy happier satisfied pleasure ~weapons bed morning death dying die "mind racing" "stopping your mind" thoughts relax anxious swing grumpy overwhelmed)


#! Ever have postpartum depression
	u: Ever_have_postpartum_depression  (ever have postpartum depression)  ^factanswer("I was never depressed")
	u: ([history diagnosed diagnosis suffer treat have] * postpartum depression) ^reuse (Ever_have_postpartum_depression)
	u: ([history diagnosed diagnosis suffer treat have feel] * ~depression * [postpartum birth]) ^reuse (Ever_have_postpartum_depression)

#! Family history of depression
	u: Family_history_of_depression  (family history of depression)  ^factanswer("No one in my family has depression.")
	u: ([~family_members family] * [history diagnosed diagnosis suffer treat] * ~depression) ^reuse (Family_history_of_depression)
	u: ([history diagnosed diagnosis suffer treat] * ~depression * [~family_members family]) ^reuse (Family_history_of_depression)

#! Do you have a history of depression
	u: Do_you_have_history_of_depression  (do you have a history of depression)  ^factanswer("I have never been depressed")
	u: ([history diagnosed diagnosis suffer treat ever] * ~depression) ^reuse (Do_you_have_history_of_depression)

#! Do you have a history of anxiety
	u: Do_you_have_history_of_anxiety  (do you have a history of anxiety)  ^factanswer("I have never been anxious.")
	u: ([history diagnosed diagnosis suffer treat] * [anxiety anxiousness anxious]) ^reuse (Do_you_have_history_of_anxiety)

#! Family history of anxiety
	u: Family_history_of_anxiety  (family history of anxiety)  ^factanswer("No one in my family has anxiety.")
	u: ([~family_members family] * [history diagnosed diagnosis suffer treat] * [anxiety anxiousness anxious]) ^reuse (Family_history_of_anxiety)
	u: ([history diagnosed diagnosis suffer treat] * [anxiety anxiousness anxious] * [~family_members family]) ^reuse (Family_history_of_anxiety)

#! Do you feel anxious
	u: Do_you_feel_anxious (do you feel anxious)  ^factanswer("Not really.")
	u: (![~family_members ~spouse anyone anybody pain] [anxious anxiety anxiousness worry "panic attack"])  ^reuse (Do_you_feel_anxious)

#! Are you depressed
	u: Are_you_depressed (are you depressed)  ^factanswer("I am fine.")
	u: ([depressed depression sad blue lonely "mental health" down hopeless]) ^reuse (Are_you_depressed)

#! Having mood changes
	u: Having_mood_changes (having mood changes)  ^factanswer("My mood is fine.")
	u: ([have experience notice] * [moody mood grumpy emotion emotional] * [swing change fluctuation behavior times swing])  ^reuse (Having_mood_changes)
	u: ([have experience notice] * [change fluctuation behavior times] * [grumpy moody mood])  ^reuse (Having_mood_changes)
	u: ([moody mood grumpy] * [~timename])  ^reuse (Having_mood_changes)

#! When did you cry last
	u: When_did_you_cry_last (when did you cry last) ^factanswer("I don't cry.")
	u: (when * [recently last] * [cry sob]) ^reuse (When_did_you_cry_last)
	u: (when * [cry sob] * [recently last]) ^reuse (When_did_you_cry_last)

#! How often do you cry
	u: How_often_do_you_cry  (how often do you cry)  ^factanswer("I do not cry very much.")
	u: ([when what how] * [often frequently much longest] * [cry sob]) ^reuse (How_often_do_you_cry)
	u: ([cry sob] more [often frequently longer] [now before]) ^reuse (How_often_do_you_cry)

#! Do you cry often
	u: Do_you_cry_often  (do you cry often)  ^factanswer("I do not cry.")
	u: ([cry sob] * [often frequently "all the time"]) ^reuse (Do_you_cry_often)
	u: ([often frequently "all the time"] * [cry sob]) ^reuse (Do_you_cry_often)
	u: ([tearful teary "tear up"]) ^reuse (Do_you_cry_often)

#! Do you feel like your mind is racing
	u: Do_you_feel_like_your_mind_is_racing (do you feel like your mind is racing)  ^factanswer("I do not.")
	u: ([feel trouble problems] * ["mind racing" "stopping your mind" thoughts relax]) ^reuse (Do_you_feel_like_your_mind_is_racing)

#! Do you feel hopeless
	u: Do_you_feel_hopeless (do you feel hopeless)  ^factanswer("I am not.")
	u: ([hopeless "not worth living" "better off dead" empty helpless worthless overwhelmed]) ^reuse (Do_you_feel_hopeless)

#! Do you feel loss of control
	u: Do_you_feel_loss_of_control (do you feel loss of control)  ^factanswer("I do not.")
	u: ([loss lack out] * [control]) ^reuse (Do_you_feel_loss_of_control)

#! Have you lost interest in activities
	u: Have_you_lost_interest_in_activities (have you lost interest in activities)  ^factanswer("I have not lost interest.")
	u: ([lost lack] * [interest enjoyment enthusiasm pleasure] * [activity things]) ^reuse (Have_you_lost_interest_in_activities)
	u: ([interest enthusiastic pleasure] * [activities things work sex]) ^reuse (Have_you_lost_interest_in_activities)

#! Do you feel guilt
	u: Do_you_feel_guilt (do you feel guilt)  ^factanswer("I do not.")
	u: ([guilt guilty blame]) ^reuse (Do_you_feel_guilt)

#! Are you stressed
	u: Are_you_stressed (are you stressed)  ^factanswer("I am not.")
	u: (![who work job] [stress overworked stressful]) ^reuse (Are_you_stressed)

#! Family history of suicide
	u: Family_history_of_suicide  (family history of suicide)  ^factanswer("No one in my family has committed suicide.")
	u: ([~family_members family] * [commit attempt intend consider thought intention] * [suicide kill harm suicidal]) ^reuse (Family_history_of_suicide)
	u: ([~family_members family] * suicidal) ^reuse (Family_history_of_suicide)

#! Are you suicidal
	u: Are_you_suicidal (are you suicidal) ^factanswer("I would never.")
	u: ([consider thought intend intention feel felt] * [suicide harm suicidal hurt]) ^reuse (Are_you_suicidal)
	u: ([harm hurt] yourself) ^reuse (Are_you_suicidal)

#! Do you think about death
	u: Do_you_think_about_death  (do you think about death) ^factanswer("I try not to.")
	u: ([think thought] * [death dying]) ^reuse (Do_you_think_about_death)

#! Do you have a weapon
	u: Do_you_have_a_weapon (do you a weapon) ^factanswer("No.")
	u: ([~weapons]) ^reuse (Do_you_have_a_weapon)


#! What is your self image
	u: What_is_self_image (what is self image) ^factanswer("I have no issues there.")
	u: ([what how] * [self image "self image"])  ^reuse (What_is_self_image)

#! Are you happy
	u: Are_you_happy (are you happy)  ^factanswer("I am happy.")
	u: ([happy happier satisfied pleasure "mental health"]) ^reuse (Are_you_happy)

#! What is your emotional state
  u: What_is_your_emotional_state (what is your emotional state) ^factanswer("My emotional state is fine.")
  u: ("emotional state") ^reuse (What_is_your_emotional_state)

